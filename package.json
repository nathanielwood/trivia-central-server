{
  "name": "trivia-central-server",
  "version": "1.0.0",
  "description": "Web API of Trivia Central",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/api",
    "check-config": "node ./config/check",
    "seed": "npm run check-config && npm run start-seed",
      "start-seed": "babel-node ./seed",
    "dev": "npm run check-config && npm run start-dev",
      "start-dev": "babel-node ./src",
    "build": "npm run check-config && npm run clean-build && npm run start-build",
      "clean-build": "rm -r ./build/ && mkdir build",
      "start-build": "babel src -d build/api && mkdir build/config && babel config/custom.js -o build/config/custom.js && mkdir build/trivia-central-lib && babel trivia-central-lib/validations.js -o build/trivia-central-lib/validations.js",
    "test": "echo todo"
  },
  "author": "Nathaniel Wood",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanielwood/trivia.git"
  },
  "devDependencies": {
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "dependencies": {
    "babel-cli": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "html-entities": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.4.8",
    "mongoose-paginate": "^5.0.0",
    "mongoose-simple-random": "^0.3.3",
    "passport": "^0.3.2",
    "passport-facebook-token": "^3.2.0",
    "passport-google-id-token": "^0.4.0",
    "passport-jwt": "^2.0.0",
    "validator": "^5.1.0"
  }
}
